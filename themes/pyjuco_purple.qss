QWidget{
	color: rgb(221, 221, 221);
	font: 10pt "Times-new-roman";
}

QMainWindow {
	background-color: rgb(45, 37, 51);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
	color: #ffffff;
	background-color: rgba(39, 32, 43, 180);
	border: 1px solid rgb(54, 43, 60);
	background-image: none;
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 2px solid rgb(174, 73, 255);
	text-align: left;
	padding-left: 8px;
	margin: 0px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Bg App */
#bgApp {	
	background-color: rgb(45, 37, 51);
	border: 1px solid rgb(54, 43, 60);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Left Menu */
#leftMenuBg {	
	background-color: rgb(39, 32, 43);
}
#topLogo {
	background-color: rgb(39, 32, 43);
	background-image: url(:/images/images/images/small_SelfPurpleLogo.png);
	background-position: centered;
	background-repeat: no-repeat;
}
#titleLeftApp { font: 63 12pt "Segoe UI Semibold"; }
#titleLeftDescription { font: 8pt "Segoe UI"; color: rgb(194, 119, 250); }

/* MENUS */
#topMenu .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 22px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#topMenu .QPushButton:hover {
	background-color: rgb(45, 37, 51);
}
#topMenu .QPushButton:pressed {	
	background-color: rgb(194, 119, 250);
	color: rgb(255, 255, 255);
}
#bottomMenu .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color:transparent;
	text-align: left;
	padding-left: 44px;
}
#bottomMenu .QPushButton:hover {
	background-color: rgb(45, 37, 51);
}
#bottomMenu .QPushButton:pressed {	
	background-color: rgb(194, 119, 250);
	color: rgb(255, 255, 255);
}
#leftMenuFrame{
	border-top: 3px solid rgb(54, 43, 60);
}

/* Toggle Button */
#toggleButton {
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color: rgb(44, 34, 49);
	text-align: left;
	padding-left: 44px;
	color: rgb(130, 111, 148);
}
#toggleButton:hover {
	background-color: rgb(45, 37, 51);
}
#toggleButton:pressed {
	background-color: rgb(194, 119, 250);
}

/* Title Menu */
#titleRightInfo { padding-left: 10px; }


