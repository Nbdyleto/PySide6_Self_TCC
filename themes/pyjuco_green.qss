QWidget{
	color: rgb(221, 221, 221);
	font: 10pt "Segoe UI";
}

QMainWindow {
	background-color: rgb(42, 48, 41);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
	color: #ffffff;
	background-color: rgba(34, 41, 34, 180);
	border: 1px solid rgb(51, 61, 50);
	background-image: none;
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 2px solid rgb(123, 250, 73);
	text-align: left;
	padding-left: 8px;
	margin: 0px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Bg App */
#bgApp {	
	background-color: rgb(42, 48, 41);
	border: 1px solid rgb(51, 61, 50);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Left Menu */
#leftMenuBg {	
	background-color: rgb(34, 41, 34);
}
#topLogo {
	background-color: rgb(34, 41, 34);
	background-image: url(:/images/images/images/small_SelfGreenLogo.png);
	background-position: centered;
	background-repeat: no-repeat;
}
#titleLeftApp { font: 63 12pt "Segoe UI Semibold"; }
#titleLeftDescription { font: 8pt "Segoe UI"; color: rgb(94, 171, 79); }

/* MENUS */
#topMenu .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 22px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#topMenu .QPushButton:hover {
	background-color: rgb(42, 48, 41);
}
#topMenu .QPushButton:pressed {	
	background-color: rgb(94, 171, 79);
	color: rgb(255, 255, 255);
}
#bottomMenu .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color:transparent;
	text-align: left;
	padding-left: 44px;
}
#bottomMenu .QPushButton:hover {
	background-color: rgb(42, 48, 41);
}
#bottomMenu .QPushButton:pressed {	
	background-color: rgb(94, 171, 79);
	color: rgb(255, 255, 255);
}
#leftMenuFrame{
	border-top: 3px solid rgb(51, 61, 50);
}

/* Toggle Button */
#toggleButton {
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color: rgb(36, 46, 35);
	text-align: left;
	padding-left: 44px;
	color: rgb(110, 147, 108);
}
#toggleButton:hover {
	background-color: rgb(42, 48, 41);
}
#toggleButton:pressed {
	background-color: rgb(94, 171, 79);
}

/* Title Menu */
#titleRightInfo { padding-left: 10px; }


