/* /////////////////////////////////////////////////////////////////////////////////////////////////
QTableWidget */
QTableWidget {	
	background-color: transparent;
	padding: 10px;
	border-radius: 0px;
	font-size: 15px;
	gridline-color: rgb(51, 61, 50);
	border-bottom: 1px solid rgb(44, 45, 61);
}
QTableWidget::item{
	background-color: rgb(51, 61, 50);
	border-color: rgb(51, 61, 50);
	margin-top: 2px;
	padding-left: 15px;
	padding-right: 5px;
	gridline-color: rgb(51, 61, 50);
}
QTableWidget::item:selected{
	background-color: rgb(94, 171, 79);
}
QTableWidget::item:hover {
    background-color: rgb(42, 48, 41);
    color : #ffffff;
}
QHeaderView::section{
	background-color: rgb(34, 41, 34);
	max-width: 30px;
	border: 1px solid rgb(51, 61, 50);
	border-style: none;
    border-bottom: 1px solid rgb(45, 48, 43);
    border-right: 1px solid rgb(45, 48, 43);
}
QTableWidget::horizontalHeader {	
	background-color: rgb(34, 41, 34);
}
QHeaderView::section:horizontal
{
    border: 1px solid rgb(34, 41, 34);
	background-color: rgb(34, 41, 34);
	padding: 3px;
	border-top-left-radius: 7px;
    border-top-right-radius: 7px;
}
QHeaderView::section:vertical
{
    border: 1px solid rgb(45, 48, 43);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Progress Bar */
QProgressBar {
    background-color: rgb(42, 48, 41);
    text-align: center;
}
QProgressBar::chunk {
    width: 1px;
    background-color: rgb(94, 171, 79);
    border: solid grey;
    border-radius: 15px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
LineEdit */
QLineEdit {
	background-color: rgb(34, 41, 34);
	border-radius: 5px;
	border: 2px solid rgb(34, 41, 34);
	padding-left: 10px;
	selection-color: rgb(255, 255, 255);
	selection-background-color: rgb(123, 250, 73);
}
QLineEdit:hover {
	border: 2px solid rgb(64, 90, 63);
}
QLineEdit:focus {
	border: 2px solid rgb(97, 127, 899);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
PlainTextEdit */
QPlainTextEdit {
	background-color: rgb(31, 36, 30);
	border-radius: 5px;
	padding: 10px;
	selection-color: rgb(255, 255, 255);
	selection-background-color: rgb(123, 250, 73);
}
QPlainTextEdit  QScrollBar:vertical {
    width: 8px;
 }
QPlainTextEdit  QScrollBar:horizontal {
    height: 8px;
 }
QPlainTextEdit:hover {
	border: 2px solid rgb(64, 90, 63);
}
QPlainTextEdit:focus {
	border: 2px solid rgb(97, 127, 899);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
ScrollBars */
QScrollBar:horizontal {
    border: none;
    background: rgb(51, 82, 64);
    height: 8px;
    margin: 0px 21px 0 21px;
	border-radius: 0px;
}
QScrollBar::handle:horizontal {
    background: rgb(94, 171, 79);
    min-width: 25px;
	border-radius: 4px
}
QScrollBar::add-line:horizontal {
    border: none;
    background: rgb(54, 86, 69);
    width: 20px;
	border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
    border: none;
    background: rgb(54, 86, 69);
    width: 20px;
	border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal
{
     background: none;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
     background: none;
}
 QScrollBar:vertical {
	border: none;
    background: rgb(51, 82, 64);
    width: 8px;
    margin: 21px 0 21px 0;
	border-radius: 0px;
 }
 QScrollBar::handle:vertical {	
	background: rgb(94, 171, 79);
    min-height: 25px;
	border-radius: 4px
 }
 QScrollBar::add-line:vertical {
     border: none;
    background: rgb(54, 86, 69);
     height: 20px;
	border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }
 QScrollBar::sub-line:vertical {
	border: none;
    background: rgb(54, 86, 69);
     height: 20px;
	border-top-left-radius: 4px;
    border-top-right-radius: 4px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }
 QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     background: none;
 }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }

/* /////////////////////////////////////////////////////////////////////////////////////////////////
CheckBox */
QCheckBox::indicator {
    border: 3px solid rgb(51, 82, 64);
	width: 15px;
	height: 15px;
	border-radius: 10px;
    background: rgb(45, 48, 43);
}
QCheckBox::indicator:hover {
    border: 3px solid rgb(59, 84, 82);
}
QCheckBox::indicator:checked {
    background: 3px solid rgb(51, 82, 64);
	border: 3px solid rgb(51, 82, 64);	
	background-image: url(:/icons/images/icons/cil-check-alt.png);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
RadioButton */
QRadioButton::indicator {
    border: 3px solid rgb(51, 82, 64);
	width: 15px;
	height: 15px;
	border-radius: 10px;
    background: rgb(45, 48, 43);
}
QRadioButton::indicator:hover {
    border: 3px solid rgb(59, 84, 82);
}
QRadioButton::indicator:checked {
    background: 3px solid rgb(92, 131, 96);
	border: 3px solid rgb(51, 82, 64);	
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
ComboBox */
QComboBox{
	background-color: rgb(31, 36, 30);
	border-radius: 5px;
	border: 2px solid rgb(34, 41, 34);
	padding: 5px;
	padding-left: 10px;
}
QComboBox:hover{
	border: 2px solid rgb(64, 90, 63);
}
QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 25px; 
	border-left-width: 3px;
	border-left-color: rgba(41, 48, 43, 150);
	border-left-style: solid;
	border-top-right-radius: 3px;
	border-bottom-right-radius: 3px;	
	background-image: url(:/icons/images/icons/cil-arrow-bottom.png);
	background-position: center;
	background-repeat: no-reperat;
 }
QComboBox QAbstractItemView {
	color: rgb(123, 250, 73);	
	background-color: rgb(34, 41, 34);
	padding: 10px;
	selection-background-color: rgb(41, 48, 43);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Sliders */
QSlider::groove:horizontal {
    border-radius: 5px;
    height: 10px;
	margin: 0px;
	background-color: rgb(51, 82, 64);
}
QSlider::groove:horizontal:hover {
	background-color: rgb(55, 62, 76);
}
QSlider::handle:horizontal {
    background-color: rgb(94, 171, 79);
    border: none;
    height: 10px;
    width: 10px;
    margin: 0px;
	border-radius: 5px;
}
QSlider::handle:horizontal:hover {
    background-color: rgb(139, 244, 139);
}
QSlider::handle:horizontal:pressed {
    background-color: rgb(123, 250, 73);
}

QSlider::groove:vertical {
    border-radius: 5px;
    width: 10px;
    margin: 0px;
	background-color: rgb(51, 82, 64);
}
QSlider::groove:vertical:hover {
	background-color: rgb(55, 62, 76);
}
QSlider::handle:vertical {
    background-color: rgb(94, 171, 79);
	border: none;
    height: 10px;
    width: 10px;
    margin: 0px;
	border-radius: 5px;
}
QSlider::handle:vertical:hover {
    background-color: rgb(139, 244, 139);
}
QSlider::handle:vertical:pressed {
    background-color: rgb(123, 250, 73);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
CommandLinkButton */
QCommandLinkButton {	
	color: rgb(123, 250, 73);
	border-radius: 5px;
	padding: 5px;
}
QCommandLinkButton:hover {	
	color: rgb(135, 255, 139);
	background-color: rgb(45, 48, 43);
}
QCommandLinkButton:pressed {	
	color: rgb(94, 171, 79);
	background-color: rgb(48, 69, 47);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Button */
#pagesContainer QPushButton {
	border: 2px solid rgb(51, 82, 64);
	border-radius: 5px;	
	background-color: rgb(51, 82, 64);
}
#pagesContainer QPushButton:hover {
	background-color: rgb(57, 80, 64);
	border: 2px solid rgb(62, 87, 69);
}
#pagesContainer QPushButton:pressed {	
	background-color: rgb(38, 51, 42);
	border: 2px solid rgb(43, 50, 61);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
QTabWidget */
QTabWidget::tab-bar {
   border: 3px solid gray;
}
QTabBar::tab {
  background: rgb(34, 41, 34);
  color: white;
  padding: 10px;
 }
QTabBar::tab:selected {
  background: rgb(94, 171, 79);
}
QTabWidget::pane {
background-color: rgb(34, 41, 34);
   border: none;
}
